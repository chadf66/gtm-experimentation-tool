gxt_profile:
  target: dev
  outputs:
    dev:
      type: {{ adapter | default('bigquery') }}
  # Credentials may be provided via environment variables or a service account JSON,
  # but can be left empty to use Application Default Credentials (ADC) when running
  # on GCP (Compute Engine, GKE, Cloud Run, etc.). Locally you can run:
  #   gcloud auth application-default login
  # or set GOOGLE_APPLICATION_CREDENTIALS to point to a service account JSON file.
  # Use dbt-style env_var placeholders so users can adapt for their environment.
      project: "{{ env_var('GCP_PROJECT', '') }}"
      dataset: "{{ env_var('BIGQUERY_DATASET', '') }}"
      credentials: "{{ env_var('GOOGLE_APPLICATION_CREDENTIALS', '') }}"
      location: "{{ env_var('BIGQUERY_LOCATION', '') }}"
    prod:
      type: {{ adapter | default('bigquery') }}
      project: "{{ env_var('GCP_PROJECT', '') }}"
      dataset: "{{ env_var('BIGQUERY_DATASET', '') }}"
      credentials: "{{ env_var('GOOGLE_APPLICATION_CREDENTIALS', '') }}"
      location: "{{ env_var('BIGQUERY_LOCATION', '') }}"
